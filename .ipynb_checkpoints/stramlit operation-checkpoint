    "import subprocess\n",
    "import webbrowser\n",
    "import time\n",
    "\n",
    "# Path to your Streamlit script\n",
    "streamlit_script_path = 'zomato.py'\n",
    "\n",
    "# Start Streamlit server\n",
    "subprocess.Popen(['streamlit', 'run', streamlit_script_path])\n",
    "\n",
    "# Wait a bit to ensure the server has time to start\n",
    "time.sleep(5)  # Adjust the sleep time as needed\n",
    "\n",
    "# Open Streamlit app in the default web browser\n",
    "webbrowser.open('http://localhost:8501')  # Default port for Streamlit\n"
   ]
